<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<AssemblyVersion>1.99.0.1012</AssemblyVersion>
		<Version>1.0.0.617</Version>
		<Authors>Carina Studio</Authors>
		<Company>Carina Studio</Company>
		<Copyright>©2021 Carina Studio</Copyright>
		<OutputType>WinExe</OutputType>
		<Nullable>enable</Nullable>
		<LangVersion>latest</LangVersion>
		<RootNamespace>Carina.PixelViewer</RootNamespace>
		<StartupObject>Carina.PixelViewer.App</StartupObject>
		<ApplicationIcon>AppIcon.ico</ApplicationIcon>
		<NoWin32Manifest>true</NoWin32Manifest>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<SatelliteResourceLanguages>en</SatelliteResourceLanguages>
		<Configurations>Debug;Release;MSStore-Debug;MSStore-Release;Debug-Windows;Release-Windows</Configurations>
	</PropertyGroup>
	
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<TargetFramework>net5.0</TargetFramework>
		<DefineConstants>DEBUG</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug-Windows|AnyCPU'">
		<TargetFramework>net5.0-windows10.0.17763.0</TargetFramework>
		<DefineConstants>DEBUG</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='MSStore-Debug|AnyCPU'">
		<TargetFramework>net5.0-windows10.0.17763.0</TargetFramework>
		<DefineConstants>DEBUG;MSSTORE</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x86'">
		<TargetFramework>net5.0</TargetFramework>
		<DefineConstants>DEBUG</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug-Windows|x86'">
		<TargetFramework>net5.0-windows10.0.17763.0</TargetFramework>
		<DefineConstants>DEBUG</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='MSStore-Debug|x86'">
		<TargetFramework>net5.0-windows10.0.17763.0</TargetFramework>
		<DefineConstants>DEBUG;MSSTORE</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
		<TargetFramework>net5.0</TargetFramework>
		<DefineConstants>DEBUG</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug-Windows|x64'">
		<TargetFramework>net5.0-windows10.0.17763.0</TargetFramework>
		<DefineConstants>DEBUG</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='MSStore-Debug|x64'">
		<TargetFramework>net5.0-windows10.0.17763.0</TargetFramework>
		<DefineConstants>DEBUG;MSSTORE</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='MSStore-Release|AnyCPU'">
		<TargetFramework>net5.0-windows10.0.17763.0</TargetFramework>
		<DefineConstants>MSSTORE</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='MSStore-Release|x64'">
		<TargetFramework>net5.0-windows10.0.17763.0</TargetFramework>
		<DefineConstants>MSSTORE</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='MSStore-Release|x86'">
		<TargetFramework>net5.0-windows10.0.17763.0</TargetFramework>
		<DefineConstants>MSSTORE</DefineConstants>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Avalonia" Version="0.10.8" />
		<PackageReference Include="Avalonia.Desktop" Version="0.10.8" />
		<PackageReference Include="Avalonia.Diagnostics" Version="0.10.8" />
		<PackageReference Include="Avalonia.Xaml.Behaviors" Version="0.10.8" />
		<PackageReference Include="CarinaStudio.AppBase.Application" Version="0.27.3.1007" />
		<PackageReference Include="CarinaStudio.AppBase.Application.Avalonia" Version="0.27.3.1007" />
		<PackageReference Include="CarinaStudio.AppBase.AutoUpdate" Version="0.27.3.1007" />
		<PackageReference Include="CarinaStudio.AppBase.Configuration" Version="0.27.3.1007" />
		<PackageReference Include="CarinaStudio.AppBase.Core" Version="0.27.3.1007" />
		<PackageReference Include="CarinaStudio.AppSuite.Core" Version="0.14.2.1013" />
		<PackageReference Condition="'$(Configuration)'=='Debug-Windows'" Include="Microsoft-WindowsAPICodePack-Shell" Version="1.1.4" />
		<PackageReference Condition="'$(Configuration)'=='Release-Windows'" Include="Microsoft-WindowsAPICodePack-Shell" Version="1.1.4" />
		<PackageReference Include="NLog" Version="4.7.11" />
		<PackageReference Condition="'$(Configuration)'=='Debug-Windows'" Include="System.Drawing.Common" Version="6.0.0-preview.5.21301.5" />
		<PackageReference Condition="'$(Configuration)'=='Release-Windows'" Include="System.Drawing.Common" Version="6.0.0-preview.5.21301.5" />
	</ItemGroup>
	
	<ItemGroup>
		<AvaloniaResource Include="AppIcon.ico" />
		<AvaloniaResource Include="Resources/RenderedImageBackground.png" />
		<AvaloniaResource Include="Resources/AppIcon_128px.png" />
		<AvaloniaResource Include="Resources/AppIcon_16px.png" />
		<AvaloniaResource Include="Resources/AppIcon_256px.png" />
		<AvaloniaResource Include="Resources/AppIcon_32px.png" />
		<AvaloniaResource Include="Resources/AppIcon_512px.png" />
		<AvaloniaResource Include="Resources/AppIcon_64px.png" />
		<AvaloniaResource Include="Resources/AppIcon.ico" />
		<AvaloniaResource Include="Strings/Default.xaml" />
		<AvaloniaResource Include="Strings/zh-TW.xaml" />
		<AvaloniaResource Include="Strings/Default-Linux.xaml" />
		<AvaloniaResource Include="Strings/zh-TW-Linux.xaml" />
		<AvaloniaResource Include="Styles/Base.axaml" />
		<AvaloniaResource Include="Styles/Dark.xaml" />
		<AvaloniaResource Include="Styles/Icons.axaml" />
		<AvaloniaResource Include="Styles/Light.xaml" />
	</ItemGroup>
	
	<ItemGroup>
		<None Update="AppIcon_128px.png">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
		<None Update="Strings/Default.xaml">
			<Generator>MSBuild:Compile</Generator>
		</None>
		<None Update="Strings/zh-TW.xaml">
			<Generator>MSBuild:Compile</Generator>
		</None>
	</ItemGroup>
	
	<ItemGroup>
		<AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
			<_Parameter1>$(AssemblyName).Test</_Parameter1>
		</AssemblyAttribute>
	</ItemGroup>
	
</Project>
