<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Carina.PixelViewer.App">

	<!-- Resources -->
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/Resources/Default.xaml"/>
				<ResourceInclude Source="/Strings/Default.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>

	<!-- Styles -->
	<Application.Styles>
		<StyleInclude Source="/Styles/Dark.xaml"/>
	</Application.Styles>
	
</Application>
